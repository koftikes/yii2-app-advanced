{
    "name": "sbs/yii2-sbs-kit",
    "description": "Yii 2 SbS Starter Kit",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "https://github.com/koftikes/yii2-sbs-kit",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/koftikes/yii2-sbs-kit/issues?state=open",
        "wiki": "https://github.com/koftikes/yii2-sbs-kit/wiki",
        "source": "https://github.com/koftikes/yii2-sbs-kit"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",

        "sbs/yii2-core-kit": "dev-master",
        "sbs/yii2-transliterator-helper": "^0.2.0",
        "sbs/yii2-seo": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/base": "^2.4.0",
        "phpunit/phpunit": "~6.5.5",
        "codeception/verify": "~1.0.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {"type": "vcs", "url": "https://github.com/koftikes/yii2-seo.git"},
        {"type": "vcs", "url": "https://github.com/koftikes/yii2-core-kit.git"}
    ]
}
